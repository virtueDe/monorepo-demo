{
  "name": "edit",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "exports": {
    ".": {
      "import": "./dist/vite-vanilla-ts-lib-starter.mjs",
      "require": "./dist/vite-vanilla-ts-lib-starter.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "main": "./dist/vite-vanilla-ts-lib-starter.cjs",
  "module": "./dist/vite-vanilla-ts-lib-starter.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --host",
    "build": "rimraf --glob build/**/* && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts && copyfiles ./package.json build",
    "build:watch": "rimraf --glob build/**/* && tsc && vite build --watch && dts-bundle-generator --config ./dts-bundle-generator.config.ts && copyfiles ./package.json build"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.5",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^9.2.4",
    "postcss": "^8.4.33",
    "postcss-scss": "^4.0.9",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2"
  }
}
